@model IEnumerable<API.Areas.RoleManager.Models.User>

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <p>
        @Html.ActionLink("Create New", "Create")
    </p>
    <table class="table">
        <tr>
            <th>
                User
            </th>
            <th>
                Action
            </th>
        </tr>

        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @item.GTAccount
                </td>
                <td>
                    @Html.ActionLink("Edit", "Edit", new { id = item.GTAccount }) |
                    @Html.ActionLink("Details", "Details", new { id = item.GTAccount }) |
                    @Html.ActionLink("Delete", "Delete", new { id = item.GTAccount })
                </td>
            </tr>
        }

    </table>
</body>
</html>
